<LinearLayout android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:id="@+id/llProgress_server"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <include
        layout="@layout/activity_title_bar"/>

<Button
    android:layout_width="200dp"
    android:layout_height="wrap_content"
    android:id="@+id/btn_progress"
    android:layout_gravity="center"
    android:layout_marginTop="90dp"
    android:background="#738b28"
    android:textColor="#FFF"
    android:text="Schedule Progress " />

    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/btn_ashistory"
        android:layout_gravity="center"
        android:layout_marginTop="30dp"
        android:background="#738b28"
        android:textColor="#FFF"
        android:text="Asset Schedule Track"/>


    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/btn_asrecord"
        android:layout_gravity="center"
        android:layout_marginTop="30dp"
        android:background="#738b28"
        android:textColor="#FFF"
        android:text="Asset Schedule Record"/>


    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/btn_overdue"
        android:layout_gravity="center"
        android:layout_marginTop="30dp"
        android:background="#738b28"
        android:textColor="#FFF"
        android:text="Overdue Schedule"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/btn_graphs"
        android:layout_gravity="center"
        android:layout_marginTop="30dp"
        android:background="#738b28"
        android:textColor="#FFF"
        android:text="Graphs"/>



    <TextView
        android:id="@+id/tv_progress_server"
        android:layout_width="match_parent"
        android:layout_height="525dp" />

</LinearLayout>







